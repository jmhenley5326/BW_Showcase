                                    <value follow=""
                                    oAuth="OAuthProperty"
                                    streamSubType="FilterStream"
                                    streamType="PublicStream"
                                    track="#bwshowcase" xsi:type="twitter:TwitterStreamListener"/>
                                </properties>
                            </activityConfig>
                        </bwext:BWActivity>
                    </tibex:eventSource>
                </tibex:receiveEvent>
            </bpws:extensionActivity>
            <bpws:extensionActivity>
