                            xmlns:input="http://tns.tibco.com/bw/activity/sendmail/xsd/input+1c5919e5-20b0-4737-a69f-3b39d1c79611+input"
                            xmlns:mail="http://ns.tibco.com/bw/palette/mail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                            <activityConfig>
